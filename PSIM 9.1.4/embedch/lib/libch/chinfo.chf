#include <chshell.h>
#include <string.h>
int chinfo(chinfo_t *info) {
   if(info == NULL)
     return -1;
   info->edition = _Chedition;
   info->releasedate = _Chreleasedate;
   info->version = _Chversion;
   info->vermajor = _Chvermajor;
   info->verminor = _Chverminor;
   info->vermicro = _Chvermicro;
   info->verbuild = _Chverbuild;
   return 0;
}
